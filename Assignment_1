//  과제 1
let scoreList = [94, 55, 67, 78, 82, 0, 100]
var grade : String

for score in scoreList {
    if score >= 90 {
        grade = "A"
    } else if score >= 80 {
        grade = "B"
    } else if score >= 70 {
        grade = "C"
    } else {
        grade = "F"
    }
    
    print ("\(score) : \(grade)")
}
